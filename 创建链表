#include <iostream>
using namespace std;
struct node{
   int data;
   node* next;
};
node* create(int a[])
{
    node *p,*pre,*head;
    head=new node;
    head->next=NULL;
    pre=head;
    for(int i=0;i<5;i++)
    {
        p=new node;
        p->data=a[i];
        p->next=NULL;
        pre->next=p;
        pre=p;
    }
    return head;
}
int main(void)
{
    int a[5]={5,9,4,6,3};
    node* L=create(a);
    L=L->next;
    while(L!=NULL)
    {
        cout<<L->data<<" ,";
        L=L->next;
    }
    return 0;
}
